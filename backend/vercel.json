{
    "version": 2,
    "builds": [
      { "src": "backend/*.js", "use": "@vercel/node" },
      { "src": "frontend/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "backend/api/$1.js" },
      { "src": "/(.*)", "dest": "frontend/$1" }
    ]
  }
  